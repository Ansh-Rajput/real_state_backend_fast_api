{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "vercel.sh && prisma generate"
      },
      "installCommand": "pip install -r requirements.txt && prisma generate"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
